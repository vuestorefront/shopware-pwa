{
  "name": "@shopware-pwa/cli",
  "version": "1.3.0",
  "description": "Shopware PWA CLI",
  "types": "build/types/types.d.ts",
  "bin": {
    "shopware-pwa": "bin/shopware-pwa"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,ts,json}",
    "lint": "tslint --config tslint.json -p . --fix",
    "clean-build": "node ./scripts/clean-build.js",
    "compile": "node ./scripts/esbuild.js",
    "copy-templates": "node ./scripts/copy-templates.js",
    "build": "yarn run format && yarn run lint && yarn run clean-build && yarn run compile && yarn run copy-templates",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "tsconfig.json",
    "tslint.json",
    "build",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "@shopware-pwa/shopware-6-client": "1.3.0",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.0",
    "gluegun": "^5.1.0",
    "md5-hex": "^3.0.1",
    "request": "^2.88.2",
    "unzipper": "^0.10.11",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@shopware-pwa/commons": "1.3.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.37",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "sort-package-json": "^1.54.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "colors": "<=1.4.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  }
}
